{{#each this}}
  <div class='container'>
    <img src='{{flags.svg}}' alt='{{name.common}}' height='35px' />
    <h1>{{name.official}}</h1>
  </div>
  <p>Capital: {{capital}}</p>
  <p>Population: {{population}}</p>
  <p>Languages:
    {{#each languages}}{{this}}{{#unless @last}}, {{/unless}}
    {{/each}}
  </p>
{{/each}}